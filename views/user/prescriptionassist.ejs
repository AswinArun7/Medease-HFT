                      <h2>Analysis Results</h2>
                      <button class="close-btn-ai" id="closeModalAI">&times;</button>
                    </div>
                    <div class="modal-body-ai" id="resultsContainerAI">
                      <!-- Dynamic Content Here -->
                    </div>
                  </div>
                </div>

                <script>
                  const fileInputAI = document.getElementById('fileInputAI');
                  const fileLabelAI = document.getElementById('fileLabelAI');
                  const formAI = document.getElementById('uploadFormAI');
                  const analyzeBtnAI = document.getElementById('analyzeBtnAI');
                  const loaderAI = document.getElementById('loaderAI');
                  const modalAI = document.getElementById('modalOverlayAI');
                  const closeModalAI = document.getElementById('closeModalAI');
                  const resultsContainerAI = document.getElementById('resultsContainerAI');
                  const historyContainerAI = document.getElementById('historyContainerAI');

                  // Load History on Start
                  renderHistoryAI();

                  fileInputAI.addEventListener('change', (e) => {
                    if (e.target.files.length > 0) {
                      fileLabelAI.innerHTML = `<span style="color: green; font-weight: bold;">${e.target.files[0].name}</span>`;
                    }
                  });

                  formAI.addEventListener('submit', async (e) => {
                    e.preventDefault();

                    if (!fileInputAI.files[0]) return;

                    // UI Loading State
                    analyzeBtnAI.disabled = true;
                    analyzeBtnAI.innerHTML = '<div style="display:block; margin:0; width:20px; height:20px; border: 2px solid #fff; border-top: 2px solid transparent; border-radius: 50%; animation: spin 1s linear infinite;"></div>';
                    loaderAI.style.display = 'block';

                    const formData = new FormData();
                    formData.append('prescription', fileInputAI.files[0]);

                    try {
                      const response = await fetch('/prescription/upload', {
                        method: 'POST',
                        body: formData
                      });

                      const data = await response.json();

                      // Check if response contains an error
                      if (!response.ok || data.error) {
                        let errorMessage = data.error || 'Analysis failed';

                        // Add suggestion if available
                        if (data.suggestion) {
                          errorMessage += '\n\n' + data.suggestion;
                        }

                        throw new Error(errorMessage);
                      }

                      // Save to History
                      addToHistoryAI(data, fileInputAI.files[0].name);

                      displayResultsAI(data);

                    } catch (error) {
                      alert('Error processing prescription: ' + error.message);
                      console.error(error);
                    } finally {
                      analyzeBtnAI.disabled = false;
                      analyzeBtnAI.innerHTML = `
                      <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                        <path d="M21 15v4a2 2 0 0 1-2 2H5a2 0 0 1-2-2v-4"></path>
                        <polyline points="17 8 12 3 7 8"></polyline>
                        <line x1="12" y1="3" x2="12" y2="15"></line>
                      </svg>`;
                      loaderAI.style.display = 'none';
                    }
                  });

                  function displayResultsAI(data) {
                    resultsContainerAI.innerHTML = '';
                    const content = generateResultHTMLAI(data);
                    resultsContainerAI.innerHTML = content;
                    modalAI.style.display = 'flex';
                  }

                  function generateResultHTMLAI(data) {
                    if (data.error) {
                      return `<p style="color: red;">Error: ${data.error}</p>`;
                    }
                    if (Array.isArray(data) && data.length > 0) {
                      return data.map(item => `
                        <div class="medicine-card">
                          <span class="medicine-name">${item.medicine_name || 'Unknown Medicine'}</span>
                          <div class="detail-row"><span class="detail-label">Purpose:</span><span class="detail-val">${item.purpose || 'N/A'}</span></div>
                          <div class="detail-row"><span class="detail-label">Dosage:</span><span class="detail-val">${item.dosage || 'N/A'}</span></div>
                          <div class="detail-row"><span class="detail-label">Schedule:</span><span class="detail-val">${item.time_schedule || 'N/A'}</span></div>
                          <div class="detail-row"><span class="detail-label">Duration:</span><span class="detail-val">${item.duration_days || 'N/A'}</span></div>
                        </div>
                      `).join('');
                    }
                    return '<p>No medicines detected or invalid format.</p>';
                  }

                  function addToHistoryAI(data, fileName) {
                    const history = JSON.parse(localStorage.getItem('prescriptionHistory') || '[]');
                    const entry = {
                      id: Date.now(),
                      fileName: fileName,
                      date: new Date().toLocaleString(),
                      data: data
                    };
                    history.unshift(entry);
                    if (history.length > 10) history.pop();
                    localStorage.setItem('prescriptionHistory', JSON.stringify(history));
                    renderHistoryAI();
                  }

                  function renderHistoryAI() {
                    const history = JSON.parse(localStorage.getItem('prescriptionHistory') || '[]');
                    historyContainerAI.innerHTML = '';

                    if (history.length === 0) {
                      historyContainerAI.innerHTML = '<p style="color: #888; text-align: center;">No history found</p>';
                      return;
                    }

                    history.forEach(item => {
                      const itemDiv = document.createElement('div');
                      itemDiv.style.background = 'rgba(255,255,255,0.6)';
                      itemDiv.style.padding = '1rem';
                      itemDiv.style.borderRadius = '8px';
                      itemDiv.style.border = '1px solid rgba(255,255,255,0.5)';
                      itemDiv.style.cursor = 'pointer';
                      itemDiv.style.display = 'flex';
                      itemDiv.style.justifyContent = 'space-between';
                      itemDiv.style.alignItems = 'center';
                      itemDiv.innerHTML = `
                        <div>
                          <strong>${item.fileName}</strong><br>
                          <small>${item.date}</small>
                        </div>
                        <span style="color: var(--primary-color)">View &rarr;</span>
                      `;
                      itemDiv.onclick = () => {
                        displayResultsAI(item.data);
                      };
                      historyContainerAI.appendChild(itemDiv);
                    });
                  }

                  closeModalAI.addEventListener('click', () => {
                    modalAI.style.display = 'none';
                  });

                  window.onclick = function (event) {
                    if (event.target == modalAI) {
                      modalAI.style.display = 'none';
                    }
                  }
                </script>
